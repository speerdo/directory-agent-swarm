# OpenRouter (all non-Google, non-Anthropic models)
OPENROUTER_API_KEY=sk-or-...

# Anthropic (Claude Sonnet via Pro sub, Claude Haiku via API)
ANTHROPIC_API_KEY=sk-ant-...

# Google (Gemini Flash, also used for Places + Custom Search)
GOOGLE_AI_API_KEY=...
GOOGLE_PLACES_API_KEY=...
GOOGLE_CSE_ID=...
GOOGLE_CSE_API_KEY=...

# SerpAPI (fallback when CSE quota exceeded)
SERPAPI_KEY=...

# Neon Database (PostgreSQL) - primary database
# Get connection string from Neon console
NEON_DB_CONNECTION_STRING=postgresql://...
# (Optional) For Neon project reference
NEON_PROJECT_ID=...
NEON_BRANCH_ID=main
# Redis (local Docker or remote)
REDIS_URL=redis://localhost:6379

# Telegram Bot
TELEGRAM_BOT_TOKEN=...
TELEGRAM_CHAT_ID=...

# Vercel (for programmatic deploys)
VERCEL_TOKEN=...
VERCEL_ORG_ID=...
